// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UsageLog {
  id          Int      @id @default(autoincrement())
  user        String
  theme       String
  fields      String?
  timeElapsed Int
  createdAt   DateTime @default(now())
}

model Pro {
  id        Int      @id @default(autoincrement())
  user      String
  expiresAt DateTime
}
